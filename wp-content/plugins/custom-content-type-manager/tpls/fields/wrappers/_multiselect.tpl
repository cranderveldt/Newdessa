<div class="cctm_element_wrapper" id="custom_field_[+id+]">
	<label for="[+id+]" class="cctm_label cctm_[+type+]" id="cctm_label_[+id+]">
		[+label+]
	</label>
	
	<select multiple="multiple" name="[+name_prefix+][+name+][]" id="[+id_prefix+][+id+]" [+extra+]>
		[+content+]
	</select>
	
	[+error_msg+]
	<span class="cctm_description">[+description+]</span>
</div>